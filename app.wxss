/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

pages {
  position: relative;
  z-index: 9999998;
  background: #fff;
}

.home {
  width: 100%;
  height: 100%;
  position: relative;
  top: 135rpx;
}

.home .down {
  float: left;
  width: 100%;
}

.home .down .tabber {
  float: left;
  width: 100%;
  margin: 100rpx 0 0 0;

}

/* 菜单选中文字颜色 */
.van-tabbar-item--active {
  color: red !important;
}